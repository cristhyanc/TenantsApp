  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Acr.UserDialogs" Version="7.0.35" />
    <PackageReference Include="Fody" Version="6.0.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="FreshMvvm" Version="3.0.0" />
    <PackageReference Include="PropertyChanged.Fody" Version="3.1.3" />
    <PackageReference Include="Syncfusion.Xamarin.Buttons" Version="17.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.Cards" Version="17.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.Core" Version="17.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.SfChart" Version="17.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.SfListView" Version="17.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.SfMaskedEdit" Version="17.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.SfNumericTextBox" Version="17.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.SfTabView" Version="17.3.0.21" />
    <PackageReference Include="Xamarin.FFImageLoading" Version="2.4.11.982" />
    <PackageReference Include="Xamarin.FFImageLoading.Forms" Version="2.4.11.982" />
    <PackageReference Include="Xamarin.FFImageLoading.Svg" Version="2.4.11.982" />
